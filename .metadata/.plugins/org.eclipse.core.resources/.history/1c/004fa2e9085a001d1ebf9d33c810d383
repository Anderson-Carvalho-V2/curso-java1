package entities;

public class Conta {
	private int numeroConta;
	private String titularConta;
	private double saldo;
	
	
	public Conta (int numeroConta, String titularConta) {
		this.numeroConta = numeroConta;
		this.titularConta = titularConta;
	}

	public String toString() {
		return "Conta: "
			+ numeroConta
			+ "Titular: "
			+ titularConta
			+ "Saldo: R$"
			+ saldo;
	}
	
	
	
}
