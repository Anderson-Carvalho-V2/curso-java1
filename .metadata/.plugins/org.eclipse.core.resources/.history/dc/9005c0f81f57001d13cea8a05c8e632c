import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		int a, b, c, d, res;
		Scanner sc = new Scanner(System.in);
		
		System.out.println("Digite um número");
		a = sc.nextInt();
		System.out.println("Digite um número");
		b = sc.nextInt();
		System.out.println("Digite um número");
		c = sc.nextInt();
		System.out.println("Digite um número");
		d = sc.nextInt();
		
		res = a * b - c * d;
		
		System.out.println("A diferencia é:" + res);
	}	
	

}
