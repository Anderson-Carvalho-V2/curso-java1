package entities;

public class Pessoa {
	private String nome;
	private int idade;
	private double altura;
	
	
	public Pessoa(String nome, int idade, double altura) {
		this.nome = nome;
		this.idade = idade;
		this.altura = altura;
	}
	
	
	
	public String toString() {
		return "Nome: "
			+ String.format("%s%n", nome)
			+ "Idade: "
			+ String.format("%d%n", idade)
			+ "Altura: "
			+ String.format("%.2f%n", altura);
	}
}
