package application;

import java.util.ArrayList;
import java.util.List;

public class Progam {

	public static void main(String[] args) {
		
		List<String> list = new ArrayList<>();
		
		list.add("Maria");
		list.add("Alex");
		list.add("Bob");
		list.add("Ana");
		list.add(2, "Marco");
		
		System.out.println(list.size());
		for(String nome: list) {
			System.out.println(nome);
		}
		
	}
}
